enter geometryParams
  set M=40
  set N=40
leave
enter problemParams
  set cfl=0.5
  set startTime=0.0
  set endTime=1.0
  set yExtent=1.0
  set diffusivity=1.0
  set buoyancy=1.0
  set forcingModel=solCXBenchmark
  set viscosityModel=solCXBenchmark
  enter initialViscosity
    set viscosityScale=1.0
  leave
  set temperatureModel=sineWave
  enter initialTemperature
    set xModes=2
    set yModes=2
    set temperatureScale=100.0
  leave
  set boundaryModel=solCXBenchmark
  set outputFile=output.h5
leave
